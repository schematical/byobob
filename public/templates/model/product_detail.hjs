{{> _header }}
{{> _navbar }}
<div class="page-header">
    <h1>{{ product.name }}<small>{{ product.desc }}</small></h1>
</div>

    <h3>Namespace</h3>
    {{ product.namespace }}

    <h3>Name</h3>
    {{ product.name }}

    <h3>Desc</h3>
    {{ product.desc }}

    <h3>Url</h3>
    {{ product.url }}

    <h3>Weight</h3>
    {{ product.weight }}

    <h3>Section</h3>
    {{ product.section }}

<a class='btn btn-default btn-sm' href='{{ product.uri }}/edit'>
    Edit
</a>
{{> _footer }}
